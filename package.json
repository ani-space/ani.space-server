{
  "name": "ani.space",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --verbose --rootDir=test",
    "test:watch": "jest --watch --rootDir=test",
    "test:cov": "jest --coverage --rootDir=test",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "typeorm": "ts-node --require tsconfig-paths/register ./node_modules/typeorm/cli",
    "migration:run": "yarn run typeorm migration:run -d ./db/typeorm.config.ts",
    "migration:generate": "yarn run typeorm -d ./db/typeorm.config.ts migration:generate",
    "migration:create": "yarn run typeorm migration:create",
    "migration:revert": "yarn run typeorm -d ./db/typeorm.config.ts migration:revert",
    "migration:show": "yarn run typeorm migration:show -d ./db/typeorm.config.ts"
  },
  "dependencies": {
    "@apollo/server": "^4.10.4",
    "@automapper/classes": "^8.8.1",
