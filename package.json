    "joi": "^17.13.0",
    "node-html-parser": "^6.1.13",
    "passport": "^0.7.0",
    "passport-google-token": "^0.1.2",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.5",
    "puppeteer": "^22.8.0",
    "radash": "^12.1.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "ts-simple-nameof": "^1.3.1",
    "typeorm": "^0.3.20"
